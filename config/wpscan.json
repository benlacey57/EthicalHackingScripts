{
  "output_file": "wpscan.txt",
  "presets": [
    {
      "name": "enumerate-users",
      "description": "Enumerates WordPress users.",
      "command": "--url {url} --enumerate u -o {output_file}"
    },
    {
      "name": "enumerate-plugins",
      "description": "Enumerates WordPress plugins.",
      "command": "--url {url} --enumerate p -o {output_file}"
    },
    {
      "name": "enumerate-themes",
      "description": "Enumerates WordPress themes.",
      "command": "--url {url} --enumerate t -o {output_file}"
    },
    {
      "name": "vulnerability-scan",
      "description": "Performs a WordPress vulnerability scan.",
      "command": "--url {url} --enumerate vp --plugins-detection mixed -o {output_file}"
    }
  ]
}